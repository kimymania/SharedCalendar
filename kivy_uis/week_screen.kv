#:kivy 2.3.1
<WeekScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: dp(10)
        spacing: dp(10)
        Label:
            id: week_label
            size_hint_y: 0.1
            halign: 'center'

        GridLayout:
            id: week_days
            cols: 7
            size_hint_y: 0.1

        GridLayout:
            id: week_grid
            cols: 7
